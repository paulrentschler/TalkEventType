<?xml version="1.0"?>
<object name="portal_skins" meta_type="Plone Skins Tool" allow_any="False"
   cookie_persistence="False"
   request_varname="plone_skin">

 <!-- Remove the TalkEventType layers -->
 <object name="talkeventtype_images"
    meta_type="Filesystem Directory View"
    directory="Products.TalkEventType:skins/talkeventtype_images"
    remove="True"/>
 <object name="talkeventtype_styles"
    meta_type="Filesystem Directory View"
    directory="Products.TalkEventType:skins/talkeventtype_styles"
    remove="True"/>
 <object name="talkeventtype_templates"
    meta_type="Filesystem Directory View"
    directory="Products.TalkEventType:skins/talkeventtype_templates"
    remove="True"/>
    
  <skin-path name="*">
	<layer name="talkeventtype_images" remove="True" />
	<layer name="talkeventtype_styles" remove="True" />
	<layer name="talkeventtype_templates" remove="True" />
  </skin-path>

</object>