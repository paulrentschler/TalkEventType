<?xml version="1.0"?>

<object name="portal_skins"
        meta_type="Plone Skins Tool"
        allow_any="False"
        cookie_persistence="False"
        request_varname="plone_skin">

 <object name="talkeventtype_images"
         meta_type="Filesystem Directory View"
         directory="TalkEventType/skins/talkeventtype_images"/>
 <object name="talkeventtype_styles"
         meta_type="Filesystem Directory View"
         directory="TalkEventType/skins/talkeventtype_styles"/>
 <object name="talkeventtype_templates"
         meta_type="Filesystem Directory View"
         directory="TalkEventType/skins/talkeventtype_templates"/>
 
 <skin-path name="*">
  <layer insert-after="*" 
         name="talkeventtype_images"/>
  <layer insert-after="*" 
         name="talkeventtype_styles"/>
  <layer insert-after="*" 
         name="talkeventtype_templates"/>
 </skin-path>

</object>